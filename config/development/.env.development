# config/development/.env.development
# Development environment configuration

# Application
NODE_ENV=development
APP_NAME=Gilty Boy Line Sheet Builder (Dev)
APP_URL=http://localhost:3000

# Airtable Configuration (Personal Access Token - PAT)
# VITE_AIRTABLE_ACCESS_TOKEN=your_personal_access_token_here
# VITE_AIRTABLE_BASE_ID=your_base_id_here
# VITE_AIRTABLE_TABLE_NAME=Products

# Example for your setup:
# VITE_AIRTABLE_ACCESS_TOKEN=patV2fXNB9gjkpy3g.34b36f91128ffefb9d292709a54b65cdc30c53c85714bf514cbdaa7612496a27
# VITE_AIRTABLE_BASE_ID=appmDNXTcMaDH4rPF
# VITE_AIRTABLE_TABLE_NAME=Products

# Development Settings
VITE_DEBUG_MODE=true
VITE_CACHE_DURATION=60000
VITE_API_TIMEOUT=10000

# Feature Flags for Development
VITE_ENABLE_DEBUG_PANEL=true
VITE_ENABLE_MOCK_DATA=false
VITE_ENABLE_PERFORMANCE_MONITORING=true