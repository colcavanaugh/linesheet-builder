{
  "name": "gilty-boy-linesheet-builder",
  "version": "1.0.0",
  "type": "module",
  "description": "Professional wholesale line sheet generator with Airtable integration",
  "main": "src/index.js",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:e2e": "playwright test",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint src --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,css,md}\"",
    "deploy": "npm run build && npm run deploy:netlify",
    "deploy:netlify": "netlify deploy --prod --dir dist",
    "audit-fix": "npm audit fix",
    "update-deps": "npm update",
    "pdf-server": "node server/pdf-server.js",
    "dev-all": "concurrently \"npm run dev\" \"npm run pdf-server\"",
    "setup-pdf": "node scripts/setup-pdf.js"
  },
  "keywords": [
    "linesheet",
    "wholesale",
    "catalog",
    "airtable",
    "pdf",
    "jewelry",
    "fashion"
  ],
  "author": "Col Cavanaugh",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.7.7",
    "cors": "^2.8.5",
    "date-fns": "^3.6.0",
    "dompurify": "^3.1.7",
    "express": "^4.21.2",
    "html2canvas": "^1.4.1",
    "jspdf": "^3.0.1",
    "marked": "^12.0.2",
    "puppeteer": "^24.15.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.48.0",
    "autoprefixer": "^10.4.20",
    "concurrently": "^8.2.2",
    "eslint": "^8.57.0",
    "postcss": "^8.4.47",
    "prettier": "^3.3.3",
    "tailwindcss": "^3.4.13",
    "typescript": "^5.6.2",
    "vite": "^6.0.7",
    "vitest": "^3.2.4"
  },
  "puppeteer": {
    "cacheDirectory": ".cache/puppeteer"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}