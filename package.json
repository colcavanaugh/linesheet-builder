{
  "name": "gilty-boy-linesheet-builder",
  "version": "1.0.0",
  "description": "Professional wholesale line sheet generator with Airtable integration",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:e2e": "playwright test",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint src --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,css,md}\"",
    "deploy": "npm run build && npm run deploy:netlify",
    "deploy:netlify": "netlify deploy --prod --dir dist",
    "audit-fix": "npm audit fix",
    "update-deps": "npm update"
  },
  "keywords": [
    "linesheet",
    "wholesale",
    "catalog",
    "airtable",
    "pdf",
    "jewelry",
    "fashion"
  ],
  "author": "Trevor Mock",
  "license": "MIT",
  "dependencies": {
    "airtable": "^0.12.2",
    "jspdf": "^2.5.2",
    "html2canvas": "^1.4.1",
    "puppeteer": "^22.15.0",
    "marked": "^12.0.2",
    "dompurify": "^3.1.7",
    "axios": "^1.7.7",
    "date-fns": "^3.6.0"
  },
  "devDependencies": {
    "vite": "^4.5.3",
    "vitest": "^1.6.0",
    "@playwright/test": "^1.48.0",
    "eslint": "^8.57.0",
    "prettier": "^3.3.3",
    "typescript": "^5.6.2",
    "tailwindcss": "^3.4.13",
    "autoprefixer": "^10.4.20",
    "postcss": "^8.4.47"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}