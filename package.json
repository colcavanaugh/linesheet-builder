{
  "name": "gilty-boy-linesheet-builder",
  "version": "1.0.0",
  "description": "Professional wholesale line sheet generator with Airtable integration",
  "main": "src/index.js",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:e2e": "playwright test",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint src --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,css,md}\"",
    "deploy": "npm run build && npm run deploy:netlify",
    "deploy:netlify": "netlify deploy --prod --dir dist"
  },
  "keywords": [
    "linesheet",
    "wholesale",
    "catalog",
    "airtable",
    "pdf",
    "jewelry",
    "fashion"
  ],
  "author": "Trevor Mock",
  "license": "MIT",
  "dependencies": {
    "airtable": "^0.12.2",
    "jspdf": "^2.5.1",
    "html2canvas": "^1.4.1",
    "puppeteer": "^21.0.0",
    "marked": "^9.0.0",
    "dompurify": "^3.0.0",
    "axios": "^1.5.0",
    "date-fns": "^2.30.0"
  },
  "devDependencies": {
    "vite": "^4.4.0",
    "vitest": "^0.34.0",
    "@playwright/test": "^1.37.0",
    "eslint": "^8.47.0",
    "prettier": "^3.0.0",
    "typescript": "^5.2.0",
    "tailwindcss": "^3.3.0",
    "autoprefixer": "^10.4.0",
    "postcss": "^8.4.0"
  }
}
